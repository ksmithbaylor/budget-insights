{
  "name": "budget-insights",
  "version": "1.0.0",
  "main": "src/index.js",
  "author": "Kevin Smith <ksmithbaylor@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "run-p frontend:dev backend:dev",
    "build": "yarn frontend:build",
    "prod": "npm-run-all frontend:build --parallel frontend:serve backend:prod",
    "test": "elm-test",
    "test:watch": "elm-test --watch",
    "frontend:dev": "ELM_DEBUGGER=false elm-app start",
    "frontend:build": "elm-app build",
    "frontend:serve": "serve -s build",
    "backend:dev": "nodemon -w src/server.js src/server.js",
    "backend:prod": "node src/server.js"
  },
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "axios": "^0.18.0",
    "caching-map": "^1.0.2",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "preact": "^8.3.1",
    "skatejs": "^5.2.4"
  },
  "devDependencies": {
    "create-elm-app": "^2.0.0-rc2",
    "elm": "0.19.0-bugfix2",
    "elm-test": "^0.19.0-beta5",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "serve": "^10.0.0"
  }
}
